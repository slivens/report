{
  "name": "react-boilerplate",
  "version": "1.2.7",
  "needFac": false,
  "facVersion": "v1.0.1",
  "description": "支持IE8的react脚手架",
  "theme": "src/theme/styles/theme-green.less",
  "engines": {
    "node": ">=6"
  },
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "preinstall": "node ./cfg/script/check",
    "clean": "node ./cfg/script/clean",
    "lang:export": "fac-tool export",
    "lang:import": "fac-tool import && fac-tool updateRequire",
    "copy:polyfill": "copyfiles -f ./polyfill/** ./dist/polyfill",
    "start": "node server.js --env=dev",
    "start:mock": "node server.js --env=dev --mock",
    "dev": "node server.js --env=dev",
    "dev:mock": "node server.js --env=dev --mock",
    "ie8": "node server.js --env=dev-ie8",
    "ie8:mock": "node server.js --env=dev-ie8 --mock",
    "dist": "npm run clean dist && npm run lang:import && npm run copy:polyfill && webpack --env=dist",
    "mock": "json-server --watch ./mock/db.json --routes ./mock/routes.json --port 8081",
    "lint": "node lint.js"
  },
  "license": "MIT",
  "dependencies": {
    "@sdp.nd/fish": "^2.6.9",
    "@sdp.nd/redux-async-promise": "0.0.2",
    "@sdp.nd/react-css-modules": "^3.6.3",
    "axios": "^0.16.1",
    "es6-promise": "4.1.1",
    "history": "^2.0.1",
    "intl": "1.1.0",
    "moment": "^2.18.1",
    "react": "0.14.9",
    "react-dom": "0.14.9",
    "react-intl": "2.2.3",
    "react-redux": "4.4.6",
    "react-router": "2.3.0",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "redux-actions": "^2.0.3",
    "redux-logger": "2.6.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@sdp.nd/eslint-config-react": "^3.0.1",
    "@sdp.nd/fac-tool": "^0.1.2",
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.26.2",
    "css-split-webpack-plugin": "^0.2.5",
    "es3ify-loader": "^0.2.0",
    "eslint": "^4.14.0",
    "eslint-loader": "^1.6.3",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "json-server": "^0.12.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "less-plugin-clean-css": "^1.5.1",
    "less-vars-to-js": "^1.2.1",
    "minimist": "^1.2.0",
    "node-sass": "^4.5.3",
    "open": "0.0.5",
    "postcss-assets": "^4.1.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "0.13.0",
    "postcss-reporter": "^3.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "url-loader": "^0.5.7",
    "webpack": "1.13.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-merge": "^4.1.0"
  }
}
